            <property name="trimStrings" value="true"/>
        </javaModelGenerator>

        <sqlMapGenerator targetPackage="com.jp.zpzc.dao.gen.mysql" targetProject="src/main/resources/">
            <property name="enableSubPackages" value="true"/>
